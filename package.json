{
    "name": "@purinton/supervisor",
    "type": "module",
    "version": "1.0.1",
    "description": "A Model Context Protocol (MCP) server template for building AI and automation tool APIs.",
    "main": "supervisor.mjs",
    "scripts": {
        "start": "node supervisor.mjs",
        "build": "webpack --config webpack.config.mjs",
        "test": "npx --node-options=\"--experimental-vm-modules --no-warnings\" jest --detectOpenHandles --silent"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/purinton/supervisor.git"
    },
    "keywords": [
        "supervisor",
        "nodejs"
    ],
    "author": "Russell Purinton <russell@purinton.us>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/purinton/supervisor/issues"
    },
    "homepage": "https://github.com/purinton/supervisor#readme",
    "devDependencies": {
        "jest": "^30.0.4"
    },
    "jest": {
        "testEnvironment": "node",
        "testMatch": [
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/?(*.)+(spec|test).mjs",
            "**/?(*.)+(spec|test).cjs"
        ]
    },
    "dependencies": {
        "@purinton/common": "^1.0.17",
        "@purinton/mcp-server": "^1.0.22",
        "dotenv": "^17.2.0"
    }
}